{
  "name": "fermier-pro",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:ci": "npx expo start",
    "start:clean": "npx expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "admin": "cd admin-web && npm start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:html": "jest --coverage --coverageReporters=html",
    "test:coverage:watch": "jest --watch --coverage",
    "test:unit": "jest --testPathIgnorePatterns=integration|e2e",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check:backend": "cd backend && npm run type-check",
    "type-check:watch": "tsc --noEmit --watch",
    "validate": "npm run lint && npm run type-check && npm test -- --passWithNoTests",
    "check-all": "npm run lint && npm run type-check && npm run format:check",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json}\" --ignore-path .gitignore",
    "deps:audit": "npm audit",
    "deps:audit:fix": "npm audit fix",
    "deps:outdated": "npm outdated",
    "deps:check": "node scripts/check-dependencies.js",
    "deps:update": "npm update",
    "deps:security": "npm audit --audit-level=moderate",
    "deps:security:check": "node scripts/check-security-vulnerabilities.js",
    "deps:verify": "npm run deps:check",
    "find:any": "node scripts/find-any-usage.js",
    "test:identify-untested": "node scripts/identify-untested-files.js",
    "test:agent": "tsx scripts/test-agent-shell-standalone.ts",
    "test:agent:pdf": "tsx scripts/test-agent-shell-standalone.ts",
    "test:pesees": "tsx scripts/test-pesees.ts",
    "generate:training-data": "tsx scripts/generate-training-data.ts",
    "save-stable": "node -e \"require('fs').copyFileSync('package.json', 'package.json.stable')\"",
    "restore-stable": "node -e \"require('fs').copyFileSync('package.json.stable', 'package.json')\" && npm install"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.24.0",
    "@react-native-community/datetimepicker": "8.6.0",
    "@react-native-picker/picker": "2.11.4",
    "@react-native-voice/voice": "^3.1.5",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/material-top-tabs": "^6.6.13",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^2.2.5",
    "bignumber.js": "^9.1.2",
    "buffer": "^6.0.3",
    "date-fns": "^3.6.0",
    "expo": "~51.0.32",
    "expo-apple-authentication": "~6.4.2",
    "expo-auth-session": "~5.5.2",
    "expo-av": "~14.0.7",
    "expo-crypto": "~13.0.2",
    "expo-dev-client": "~4.0.26",
    "expo-document-picker": "~12.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.10",
    "expo-haptics": "~13.0.1",
    "expo-image": "~1.13.0",
    "expo-image-manipulator": "~12.0.5",
    "expo-image-picker": "~15.1.0",
    "expo-keep-awake": "~13.0.2",
    "expo-localization": "~15.0.3",
    "expo-location": "~17.0.1",
    "expo-notifications": "~0.28.16",
    "expo-print": "~13.0.1",
    "expo-secure-store": "^15.0.8",
    "expo-sensors": "~13.0.9",
    "expo-sharing": "~12.0.1",
    "expo-speech": "~12.0.2",
    "expo-status-bar": "~1.12.1",
    "expo-web-browser": "~13.0.3",
    "i18n-js": "^4.4.3",
    "lodash": "^4.17.21",
    "lucide-react-native": "^0.446.0",
    "make-plural": "^6.2.2",
    "normalizr": "^3.6.2",
    "react": "18.2.0",
    "react-native": "0.83.1",
    "react-native-calendars": "^1.1305.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-modal": "^13.0.1",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~3.19.5",
    "react-native-safe-area-context": "4.14.1",
    "react-native-screens": "3.37.0",
    "react-native-svg": "15.15.1",
    "react-native-tab-view": "^3.5.2",
    "react-native-uuid": "^2.0.2",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/react": "~18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "babel-preset-expo": "~11.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.4",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.3.3"
  },
  "overrides": {
    "js-yaml": ">=4.1.1",
    "@expo/config-plugins": "~8.0.0"
  },
  "private": true
}
